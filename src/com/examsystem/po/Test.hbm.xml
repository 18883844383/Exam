<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="com.examsystem.po.Test" table="test"><!-- 每个class对应一个持久化对象 -->
		<id name="id" type="int"><!-- id元素用来定义主键标识,并指定主键生成策略 -->
			<column name="id"></column>
			<generator class="native"></generator>
		</id>
		
		<property name="title"></property>
		
		<property name="optionA"></property>
		
		<property name="optionB"></property>

		<property name="optionC"></property>
		
		<property name="optionD"></property>

		<property name="answer"></property>
		
		<property name="prase"></property>
		
		<property name="diffculty"></property>
		
		<property name="objectId"></property>
		
		<property name="testCenter"></property>
</class>
</hibernate-mapping>
